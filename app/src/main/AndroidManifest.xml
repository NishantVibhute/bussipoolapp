<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bussinesspoolapp.bussipool">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Home" />
        <activity android:name=".SignUp" />
        <activity android:name=".SchemeJoin" />
        <activity android:name=".CreateUser" />
        <activity android:name=".SchemeHome" />
        <activity android:name=".Promote" />
        <activity android:name=".ContactsPickerActivity" />
        <activity android:name=".Chat" />
        <activity android:name=".Joining" />
        <activity android:name=".Video" />
        <activity android:name=".Account" />
        <activity android:name=".Payment" />
        <activity android:name=".SentMessages"></activity>
    </application>

</manifest>